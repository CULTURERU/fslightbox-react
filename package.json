{
  "name": "fslightbox-react",
  "version": "0.8.0",
  "description": "React version of Fullscreen Lightbox",
  "main": "main.js",
  "scripts": {
    "test": "jest",
    "build": "webpack --mode production --config webpack-prod.config.js --display-modules",
    "start": "webpack-dev-server --mode development --open"
  },
  "repository": {
    "type": "git",
    "_url": "git+https://github.com/PiotrZdziarski/React-FsLightbox-Basic.git"
  },
  "jest": {
    "setupFiles": [
      "./setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|less|sass|scss|jpg|jpeg)$": "<rootDir>/tests/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/tests/__mocks__/fileMock.js"
    },
    "collectCoverage": false
  },
  "keywords": [
    "react lightbox",
    "fslightbox",
    "Fullscreen Lightbox",
    "react",
    "lightbox",
    "javascript"
  ],
  "author": "Piotr Zdziarski",
  "license": "MIT",
  "bugs": {
    "_url": "https://github.com/PiotrZdziarski/React-FsLightbox-Basic/issues"
  },
  "homepage": "https://fslightbox.com/",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "core-js": "^3.0.1",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.7.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
