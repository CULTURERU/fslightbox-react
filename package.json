{
  "name": "fslightbox-react",
  "version": "0.0.3",
  "description": "React version of Fullscreen Lightbox",
  "main": "src/",
  "scripts": {
    "test": "jest",
    "build": "webpack --mode production --config webpack-prod.config.js",
    "start": "webpack-dev-server --mode development --open --port 3000 --host 192.168.0.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PiotrZdziarski/React-FsLightbox-Basic.git"
  },
  "jest": {
    "setupFiles": [
      "./setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    },
    "moduleNameMapper": {
      "\\.(css|less|sass|scss|jpg|jpeg)$": "<rootDir>/tests/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/tests/__mocks__/fileMock.js"
    },
    "collectCoverage": true
  },
  "keywords": [
    "react lightbox",
    "fslightbox",
    "Fullscreen Lightbox",
    "react",
    "lightbox",
    "javascript"
  ],
  "author": "Piotr Zdziarski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PiotrZdziarski/React-FsLightbox-Basic/issues"
  },
  "homepage": "https://fslightbox.com/",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.0",
    "enzyme-to-json": "^3.3.5",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.1",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {}
}
